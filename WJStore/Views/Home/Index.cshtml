@model IEnumerable<Product>
@{
    ViewBag.Title = "WooJ Corporation Store";
}
<div id="promotion">
</div>

<h3><em>Most</em> Populer This Week</h3>

<ul id="product-list">
    @foreach (var product in Model)
    {
        <li><a href="@Url.Action("Details", "Store",
                new { id = product.ProductId })">

            <img alt="@product.Title" src="@product.ProductArtUrl" />
            <span>@product.Title</span> </a>
        </li>
    }
</ul>